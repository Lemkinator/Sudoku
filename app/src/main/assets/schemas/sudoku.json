{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "size": {
      "type": "integer"
    },
    "difficulty": {
      "type": "integer"
    },
    "modeLevel": {
      "type": "integer"
    },
    "regionalHighlightingUsed": {
      "type": "boolean"
    },
    "numberHighlightingUsed": {
      "type": "boolean"
    },
    "eraserUsed": {
      "type": "boolean"
    },
    "isChecklist": {
      "type": "boolean"
    },
    "isReverseChecklist": {
      "type": "boolean"
    },
    "checklistNumber": {
      "type": "integer"
    },
    "hintsUsed": {
      "type": "integer"
    },
    "notesMade": {
      "type": "integer"
    },
    "errorsMade": {
      "type": "integer"
    },
    "created": {
      "type": "string"
    },
    "updated": {
      "type": "string"
    },
    "seconds": {
      "type": "integer"
    },
    "fields": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Field"
      }
    }
  },
  "required": [
    "id",
    "size",
    "difficulty",
    "modeLevel",
    "regionalHighlightingUsed",
    "numberHighlightingUsed",
    "eraserUsed",
    "isChecklist",
    "isReverseChecklist",
    "checklistNumber",
    "hintsUsed",
    "notesMade",
    "errorsMade",
    "created",
    "updated",
    "seconds",
    "fields"
  ],
  "definitions": {
    "Field": {
      "type": "object",
      "properties": {
        "index": {
          "type": "integer"
        },
        "value": {
          "type": [
            "integer",
            "null"
          ]
        },
        "solution": {
          "type": "integer"
        },
        "notes": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        },
        "given": {
          "type": "boolean"
        },
        "hint": {
          "type": "boolean"
        }
      },
      "required": [
        "index",
        "value",
        "solution",
        "notes",
        "given",
        "hint"
      ]
    }
  }
}
