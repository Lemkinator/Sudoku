{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":disableDependencyDashboard",
    ":renovatePrefix",
    ":configMigration"
  ],
  "ignoreDeps": [
    "io.github.lemkinator:common-utils"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "digest"
      ],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    },
    {
      "matchPackagePrefixes": [
        "sesl.androidx"
      ],
      "automerge": false
    }
  ]
}
